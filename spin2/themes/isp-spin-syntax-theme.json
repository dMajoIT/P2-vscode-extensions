{
  "$schema": "vscode://schemas/color-theme",
  "name": "Spin2 Ironsheep Syntax",
  "type": "dark",
  "colors": {
    "activityBar.background": "#444444",
    "activityBar.border": "#2F8FBF",
    "activityBar.foreground": "#888888",
    "activityBarBadge.background": "#2F8FBF",
    "activityBarBadge.foreground": "#FFFFFF",
    "editor.background": "#222222",
    "editor.foreground": "#aaaaaa",
    "editorError.border": "#BF2F2F",
    "editorLineNumber.foreground": "#444444",
    "panel.border": "#2F8FBF",
    "panelTitle.activeBorder": "#2F8FBF",
    "panelTitle.activeForeground": "#2F8FBF",
    "panelTitle.inactiveForeground": "#444444",
    "sideBar.background": "#222222",
    "sideBar.border": "#888888",
    "sideBar.foreground": "#AAAAAA",
    "sideBarSectionHeader.background": "#444444",
    "sideBarSectionHeader.foreground": "#000000",
    "sideBarTitle.foreground": "#AAAAAA",
    "terminal.ansiBlue": "#2F8FBF",
    "terminal.ansiBrightBlue": "#007FBF",
    "terminal.ansiBrightCyan": "#00BF7F",
    "terminal.ansiBrightGreen": "#7FBF00",
    "terminal.ansiBrightMagenta": "#7F00BF",
    "terminal.ansiBrightRed": "#BF0040",
    "terminal.ansiBrightYellow": "#BF7F00",
    "terminal.ansiCyan": "#2FBF8F",
    "terminal.ansiGreen": "#8FBF2F",
    "terminal.ansiMagenta": "#8F2FBF",
    "terminal.ansiRed": "#BF3060",
    "terminal.ansiYellow": "#BF8F2F",
   },
  "tokenColors": [
    {
      "name": "Non-documentation Comment",
      "scope": [
        "comment.block",
        "comment.line",
      ],
      "settings": {
        "foreground": "#538054",
        "fontStyle": "italic",
      },
    },
    {
      "name": "Documentation Comment",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation",
      ],
      "settings": {
        "foreground": "#5db55d",
      },
    },
    {
      "name": "Immediate",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ff4444",
      },
    },
    {
      "name": "Constant",
      "scope": [
        "constant.language",
        "keyword.operator.binary.spin2",
        "keyword.operator.field.spin2",
        "keyword.operator.float.spin2",
        "keyword.operator.unary.spin2",
      ],
      "settings": {
        "foreground": "#8a358a",
      },
    },
    {
      "name": "Numeric",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#3e9096",
      },
    },
    {
      "name": "Object Variables",
      "scope": ["entity.name.object"],
      "settings": {
        "foreground": "#7A81FF",
      },
    },
    {
      "name": "Invalid Use",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff0000",
      },
    },
    {
      "name": "Section",
      "scope": ["keyword.block"],
      "settings": {
        "foreground": "#873d54",
        "fontStyle": "bold",
      },
    },
    {
      "name": "Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#b57bb0",
      },
    },
    {
      "name": "Control",
      "scope": ["keyword.control.include"],
      "settings": {
        "foreground": "#c659bd",
      },
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#b567ae",
      },
    },
    {
      "name": "Other",
      "scope": ["keyword.other"],
      "settings": {
        "foreground": "#2f8fbf",
      },
    },
    {
      "name": "pasm control",
      "scope": ["keyword.pasm.control"],
      "settings": {
        "foreground": "#9b3834",
      },
    },
    {
      "name": "pasm instructions",
      "scope": ["keyword.pasm.instruction"],
      "settings": {
        "foreground": "#9b3834",
      },
    },
    {
      "name": "Pasm Operator",
      "scope": ["keyword.pasm.operators"],
      "settings": {
        "foreground": "#b567ae",
      },
    },
    {
      "name": "spin operators",
      "scope": ["keyword.spin.operators"],
      "settings": {
        "foreground": "#831799",
      },
    },
    {
      "name": "Filename",
      "scope": ["meta.object.filename"],
      "settings": {
        "foreground": "#eb6077",
      },
    },
    {
      "name": "Method Return Variable",
      "scope": ["meta.return-type.spin"],
      "settings": {
        "foreground": "#3e8b95",
      },
    },
    {
      "name": "pasm storage modifiers",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#7172ac",
      },
    },
    {
      "name": "storage types",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#468a9d",
      },
    },
    {
      "name": "String",
      "scope": ["string.quoted.double"],
      "settings": {
        "foreground": "#ba4343",
      },
    },
    {
      "name": "spin methods",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#c1c07d",
      },
    },
    {
      "name": "debug method",
      "scope": ["support.function.debug"],
      "settings": {
        "foreground": "#ffd493",
      },
    },
    {
      "name": "debug functions",
      "scope": ["support.function.debug.methods"],
      "settings": {
        "foreground": "#b96c00",
      },
    },
    {
      "name": "Variable",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#8a358a",
      },
    },
    {
      "name": "Cog Register",
      "scope": ["variable.language.cog.register"],
      "settings": {
        "foreground": "#9437FF",
      },
    },
    {
      "name": "Simple Variables",
      "scope": ["variable.name.readwrite"],
      "settings": {
        "foreground": "#7A81FF",
      },
    },
   ],
}
