{
    "name": "spin2",
    "displayName": "Spin2",
    "description": "Spin2 sytax hightlighting and outline support",
    "icon": "images/Propeller.ico",
    "version": "0.1.2",
    "publisher": "Cluso99",
    "repository": {
        "type": "git",
        "url": "https://github.com/ironsheep/P2-vscode-support"
    },
    "author": "Cluso99",
    "galleryBanner": {
        "color": "#003060",
        "theme": "dark"
    },
    "preview": true,
    "keywords": [
        "spin",
        "spin2",
        "propeller",
        "propeller ii",
        "parallax"
    ],
    "license": "MIT",
    "main": "out/spin2.extension.js",
    "activationEvents": [
        "onLanguage:spin2"
    ],
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "language": "spin2",
                "scopeName": "source.spin2",
                "path": "syntaxes/spin2.tmLanguage.json"
            }
        ],
        "languages": [
            {
                "id": "spin2",
                "aliases": [
                    "spin2",
                    "Spin2",
                    "SPIN2"
                ],
                "extensions": [
                    ".spin2"
                ],
                "configuration": "./spin2.language-configuration.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile"
    },
    "devDependencies": {
        "@types/vscode": "^1.50.0",
        "js-yaml": "^3.14.0",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "vsce": "^1.81.1"
    }
}
